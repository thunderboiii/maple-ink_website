<section class="contact">
    <div class="contact-info">
        <p class="description">Feel free to contact us at any time! We're always happy to discuss new project.</p>
        <div class="adresse info-box">
            <i class='bx bxs-map-alt' ></i>
            <div class="details">
                <p class="title">Where to find us</p>
                <p>Luise-Rappaport-Weg 13, 48161 Münster</p>
            </div>
        </div>
        <div class="mail info-box">
            <i class='bx bxs-envelope' ></i>
            <div class="details">
                <p class="title">Mail us</p>
                <a href="mailto:info@maple-ink.com">info@maple-ink.com</a>
            </div>
        </div>
        <div class="phone info-box">
            <i class='bx bxs-phone-outgoing' ></i>
            <div class="details">
                <p class="title">Call us</p>
                <a href="tel:0152 / 513 69 734">0152 / 513 69 734</a>
            </div>
        </div>
    </div>

    <form action="https://formsubmit.co/emil@maple-ink.com" method="POST">
        <input type="hidden" name="_next" value="/thank-you">
        <input type="hidden" name="_subject" value="New submission!">
        <div class="question">
            <p>Which service do you want?</p>
            <div class="input-holder">
                <div class="check">
                    <input type="checkbox" name="service" id="corporate_branding"></iunput>
                    <label for="corporate_branding">Corporate Branding</label>
                </div>

                <div class="check">
                    <input type="checkbox" name="service" id="personal_branding"></iunput>
                    <label for="personal_branding">Personal Branding</label>
                </div>
                
                <div class="check">
                    <input type="checkbox" name="service" id="corporate_web"></iunput>
                    <label for="corporate_web">Corporate Webdesign</label>
                </div>

                <div class="check">
                    <input type="checkbox" name="service" id="personal_web"></iunput>
                    <label for="personal_web">Portfolio Webdesign</label>
                </div>

                {# <div class="check">
                    <input type="checkbox" name="branding" id="ecommerce"></iunput>
                    <label for="ecommerce">E-Commerce</label>
                </div> #}

                <div class="check">
                    <input type="checkbox" name="service" id="maintenance"></iunput>
                    <label for="maintenance">Maintenance</label>
                </div>

                {# <div class="check">
                    <input type="checkbox" name="branding" id="product_branding"></iunput>
                    <label for="product_branding">Product Branding</label>
                </div> #}

                <div class="check">
                    <input type="checkbox" name="service" id="social_media"></iunput>
                    <label for="social_media">Social Media</label>
                </div>

                <div class="check">
                    <input type="checkbox" name="service" id="social_ads"></iunput>
                    <label for="social_ads">Social Advertisement</label>
                </div>

                {# <div class="check">
                    <input type="checkbox" name="branding" id="pr"></iunput>
                    <label for="pr">Public Relations</label>
                </div> #}
            </div>
        </div>

        <div class="question">
            <p>How high is your budget?</p>

            <div class="input-holder">
                <div class="check">
                    <input type="radio" name="budget" id="1k"></iunput>
                    <label for="1k">1k to 5k</label>
                </div>

                <div class="check">
                    <input type="radio" name="budget" id="5k"></iunput>
                    <label for="5k">5k to 15k</label>
                </div>

                <div class="check">
                    <input type="radio" name="budget" id="15k"></iunput>
                    <label for="15k">15k to 25k</label>
                </div>

                <div class="check">
                    <input type="radio" name="budget" id="25k"></iunput>
                    <label for="25k">25k to 50k</label>
                </div>

                <div class="check">
                    <input type="radio" name="budget" id="50k"></iunput>
                    <label for="50k">50k and above</label>
                </div>
            </div>
        </div>

        <div class="question">
            <p>Tell us a bit about your project</p>
            <div class="input-holder">
                <textarea name="project information" id="" cols="30" rows="10" placeholder="What is your project about?" required></textarea>
            </div>
        </div>

        <div class="question">
            <p>Tell us a bit about yourself</p>
            <div class="input-holder yourself">
                <div class="name text-holder">
                    <input type="text" name="name" id="" placeholder="Your Name" required>
                    <input type="text" name="company" id="" placeholder="Your Company" required>
                </div>
                <div class="email  text-holder">
                    <input type="text" name="e-mail" id="" placeholder="Your E-mail" required>
                    <input type="text" name="phone" id="" placeholder="Phone (Optional)">
                </div>
            </div>
        </div>

        <hr>
        <p class="disclaimer" ><input type="checkbox" name="disclaimer" id="disclaimer" required>By clicking the checkbox to the left and submitting the form you agree to our <a href="/tos">Terms of Service</a> as well as <a href="/data-protection">Data Protection</a>.</p>

        <button type="submit">Submit</button>
        
    </form>
